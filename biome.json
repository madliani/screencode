{
    "$schema": "https://biomejs.dev/schemas/1.9.3/schema.json",
    "files": {
        "ignore": [],
        "ignoreUnknown": false
    },
    "formatter": {
        "enabled": true,
        "indentStyle": "space"
    },
    "javascript": {
        "formatter": {
            "quoteStyle": "double"
        }
    },
    "linter": {
        "enabled": true,
        "rules": {
            "recommended": true
        }
    },
    "organizeImports": {
        "enabled": true
    },
    "vcs": {
        "clientKind": "git",
        "enabled": true,
        "useIgnoreFile": true
    }
}